<!--<view class="pic-list">-->
    <view class="img-wrap">
        <view class="img-row"
            wx:for="{{props.srcArr}}"
            wx:for-index="index"
            wx:key="{{index}}"
            wx:for-item="src"
        >   
            <image wx:if="{{props.local}}" id="{{src}}" bindtap="preimglocal" 
                style="width:100%;height:100%;background-color: #eeeeee;" 
                mode="aspectFill" src="{{src}}"></image>
            <image wx:if="{{!props.local}}" id="{{props.host+src.url}}" bindtap="preimg" 
                style="width:100%;height:100%;background-color: #eeeeee;" 
                mode="aspectFill" src="{{props.host+src.url}}"></image>
        </view>
    </view>
<!--</view>-->
